1. Absolute Value --c++
int absolute (int I){
   if(I<0){
   return -I;
   }
   else{
   return I;
   }
}

2. Convert Celsius To Fahrenheit -C++
double cToF(int C)
{
float F;
    F = (float)(Cx9/5 + 32);
    return F;
}

3. Quadratic Equation Roots --c++
#include<math.h>
class Solution {
  public:
    vector<int> quadraticRoots(int a, int b, int c) {
    float x1,x2, d;
    d=(float)(sqrt(b*b -4*a*c));
    x1= (float)((-b + d)/2a);
    x1= (float)((-b - d)/2a);
    if(d<0){
        return 0;
    }
    elseif(d=0){
        return x1,x2;
    }
    else{
    return x1,x2;
    }
};


